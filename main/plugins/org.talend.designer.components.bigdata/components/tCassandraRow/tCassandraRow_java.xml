        </PARAMETER>
        <PARAMETER
            NAME="API_TYPE"
            FIELD="CLOSED_LIST"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION == 'CASSANDRA_2_0_0')"
